                        android:text="Number of Persons on the Last working day."/>
                    <LinearLayout
                        style="@style/hliner"
                        android:weightSum="3">
                        <LinearLayout
                            style="@style/vliner"
                            android:layout_weight="1">
                            <TextView
                                style="@style/txt_lbl"
                                android:text="Male" />
                            <EditText
                                android:id="@+id/Male308"
                                style="@style/edit_form_text"
                                android:digits="0123456789"
                                android:maxLines="1"
                                android:maxLength="4"
                                android:inputType="number"/>
                        </LinearLayout>
                        <LinearLayout
                            style="@style/vliner"
                            android:layout_weight="1">
                            <TextView
                                style="@style/txt_lbl"
                                android:text="Female"/>
                            <EditText
                                android:id="@+id/Female308"
                                style="@style/edit_form_text"
                                android:digits="0123456789"
                                android:maxLines="1"
                                android:maxLength="4"
                                android:inputType="number"/>
                        </LinearLayout>
                        <LinearLayout
                            style="@style/vliner"
                            android:layout_weight="1">
                            <TextView
                                style="@style/txt_lbl"
                                android:text="Total"/>
                            <EditText
                                android:id="@+id/Total308"
                                android:enabled="false"
                                style="@style/edit_form_text"
                                android:digits="0123456789"
                                android:maxLines="1"
                                android:maxLength="11"
                                android:inputType="number"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        style="@style/hliner"
                        android:weightSum="3">
                        <LinearLayout
                            style="@style/vliner"
                            android:layout_weight="1">
                            <TextView
                                style="@style/txt_lbl"
                                android:text="Wages / Salaries"/>
                            <EditText
                                android:id="@+id/Wage308"
                                style="@style/edit_form_text"
                                android:digits="0123456789"
                                android:maxLines="1"
                                android:maxLength="11"
                                android:hint="Rs. in '000'"
                                android:inputType="number"/>
                        </LinearLayout>
                        <LinearLayout
                            style="@style/vliner"
                            android:layout_weight="1">
                            <TextView
                                style="@style/txt_lbl"
                                android:text="Cash Payments"/>
                            <EditText
                                android:id="@+id/Cash308"
                                style="@style/edit_form_text"
                                android:digits="0123456789"
                                android:maxLines="1"
                                android:maxLength="11"
                                android:hint="Rs. in '000'"
                                android:inputType="number"/>
                        </LinearLayout>
                        <LinearLayout
                            style="@style/vliner"
                            android:layout_weight="1">
                            <TextView
                                style="@style/txt_lbl"
                                android:text="Non-Cash Benefits"/>
                            <EditText
                                android:id="@+id/kind308"
                                style="@style/edit_form_text"
                                android:digits="0123456789"
                                android:maxLines="1"
                                android:maxLength="11"
                                android:hint="Rs. in '000'"
                                android:inputType="number"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <include layout="@layout/layout_form_buttons" />
            </LinearLayout>
        </ScrollView>
        <include
            android:id="@+id/footer"
            layout="@layout/footer"/>
    </RelativeLayout>

    <com.google.android.material.navigation.NavigationView
        android:id="@+id/nav_view"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        app:menu="@menu/main_menu"
        app:headerLayout="@layout/mheader"/>

</androidx.drawerlayout.widget.DrawerLayout>